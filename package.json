    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "daterangepicker": "^3.0.5",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "0.4.3",
    "moment": "^2.23.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "owl.carousel": "^2.3.4",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-app-polyfill": "^0.1.3",
    "react-datepicker": "^2.8.0",
    "react-datetime": "^2.16.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.9.0",
    "react-google-maps": "^9.4.5",
    "react-hot-loader": "^4.12.11",
    "react-inlinesvg": "^0.8.4",
    "react-input-mask": "^2.0.4",
    "react-owl-carousel": "^2.3.1",
    "react-photoswipe": "^1.3.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.25.2",
    "react-svg": "^7.2.14",
    "react-yandex-maps": "2.2.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "semantic-ui-react": "^0.85.0",
    "slick-carousel": "^1.8.1",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node webpack/start.js",
    "build": "node webpack/build.js",
    "flow": "flow ./source",
    "flow-init": "flow init",
    "lint": "eslint ./source/ --ext .js,.jsx"
  },
  "lint-staged": {
    "*.{js, jsx, scss, json}": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-vvdev-rn": "^1.0.5",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.96.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.{js,jsx,ts,tsx}",
      "!source/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/source/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/source/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_dules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
